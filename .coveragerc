[run]
source = src
omit =
    src/ui/*
    src/services/*
    src/models/concretos/sofacama.py
    src/models/concretos/comedor.py
    src/models/concretos/mesa.py
    src/models/concretos/cama.py
    src/models/concretos/silla.py
    src/models/concretos/sofa.py
    src/models/mueble.py
    src/main.py
    */__pycache__/*
    */tests/*
    */venv/*
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == __main__:
    pass

fail_under = 80